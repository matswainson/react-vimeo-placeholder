{
  "name": "react-vimeo-placeholder",
  "version": "1.0.2",
  "description": "Embed Vimeo player with a custom placeholder image and play button",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --config webpack.config --progress --colors -p",
    "lint": "eslint src/**/*.tsx",
    "storybook": "start-storybook",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matswainson/react-vimeo-placeholder.git"
  },
  "keywords": [
    "react",
    "vimeo",
    "embed",
    "player",
    "placeholder"
  ],
  "author": "Mat Swainson <mat@matswainson.com> (http://matswainson.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matswainson/react-vimeo-placeholder/issues"
  },
  "homepage": "https://github.com/matswainson/react-vimeo-placeholder#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/react": "^6.0.21",
    "@types/classnames": "^2.2.9",
    "@types/events": "^3.0.0",
    "@types/jest": "^25.1.1",
    "@types/jsonp": "^0.2.0",
    "@types/react": "^16.9.19",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^25.1.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "jsonp": "^0.2.1"
  }
